<h1>My Tasks for the day</h1>
<h2>_______________________</h2>


<ul>
  <% @tasks.each do |task| %>
    <li>
      <%= link_to task.title, task_path(task) %>
      <%= link_to "Edit task", edit_task_path(task) %>
      <%= button_to "Delete", task_path(task), method: :delete %>
    </li>
  <% end %>
</ul>
<%= link_to "Add a new task", new_task_path %>
